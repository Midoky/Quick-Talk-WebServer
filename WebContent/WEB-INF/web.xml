<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>WebTools</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>com.servlet.UserLogin</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>GetMsgs</servlet-name>
    <servlet-class>com.servlet.GetMsgs</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>ReiceveMsg</servlet-name>
    <servlet-class>com.servlet.ReiceveMsg</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateRoom</servlet-name>
    <servlet-class>com.servlet.CreateRoom</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserRegister</servlet-name>
    <servlet-class>com.servlet.UserRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReiceveMsg</servlet-name>
    <url-pattern>/ReiceveMsg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateRoom</servlet-name>
    <url-pattern>/CreateRoom</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>GetMsgs</servlet-name>
    <url-pattern>/GetMsgs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserRegister</servlet-name>
    <url-pattern>/UserRegister</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>